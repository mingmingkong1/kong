#!/usr/bin/env groovy



properties([
        parameters([
              
              
                password(defaultValue: '', description: '', name: 'password'),
                string(defaultValue: '', description: '', name: 'user'),
                
        ])
])


pipeline {
    agent any

    stages {
        stage('clonefirst'){
            steps{
                echo $params.user
        }
        }
       
        
     }
}
    
